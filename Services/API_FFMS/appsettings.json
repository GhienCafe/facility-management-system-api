{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ENV_ENVIRONMENTS": {
    "CONNECTION_STRING": "YOUR_CONNECTION_STRING",
    "JWT_ACCESS_TOKEN_SECRET": "YOUR_ACCESS_TOKEN",
    "JWT_RESET_TOKEN_SECRET": "YOUR_RESET_TOKEN",
    "JWT_AUDIENCE": "YOUR_AUDIENCE",
    "JWT_ISSUER": "YOUR_ISSUE",
    "JWT_ACCESS_TOKEN_EXPIRES": 0,
    "JWT_RESET_TOKEN_EXPIRES": 0
  }
}
